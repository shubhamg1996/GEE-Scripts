// Bands to select from the image
var bands = ['B1','B2', 'B3', 'B4', 'B5', 'B7'];

// Visualization parameters
var viz = {bands: ['B3', 'B2', 'B1'], max: 0.5, gamma: 2};
var polygons = non.merge(built);
print(polygons);
// Getting best image tile for the area near point
var image = ee.Image(l8.filterBounds(non).filterDate('2017-01-01','2017-10-01').sort('CLOUD_COVER').first()).select(bands);

// clipping the district boundary from image tile
image = image.clip(korba);

// adding the best clipped image for the district
Map.addLayer(image,viz,'korba_image');

// Passing training data for the classifier
// var polygons = built.merge(non);
var polygons = ee.FeatureCollection('ft:1CCbCTHWaYw7GcbN2pNO02QIdhEeBElKUMkoooRUi').remap([1, 2], [0, 1], 'class');
// creating training variable to be passed to the classifier
var training = image.select(bands).sampleRegions({
  collection: polygons,
  properties: ['class'],
  scale: 30
});
// training for the classifier ( CART classifier )

// var trained = ee.Classifier.cart().train({
//   features:training,
//   classProperty:'class',
//   inputProperties:bands
// });

// training for the classifier ( Random Forest classifier used with 10 trees )
var trained = ee.Classifier.randomForest(10).train(training, 'class', bands);

// classifying the whole image for the district out of the training that classifier got
var classified = image.classify(trained);

// adding the classified image to the map
Map.addLayer(classified.updateMask(classified.not()), {min: 0, max: 1, palette: ['FF0000', '00FF00']},
  'RandomForest classification');
  

var SCALE = Map.getScale();

// reducing all the classified pixels to vectors
var pixel_polys = classified.updateMask(classified.not()).reduceToVectors({
  geometry: korba,
  scale: 24,
  maxPixels: 1e10,
});

// adding area to each reduced vector
pixel_polys = pixel_polys.map(function(feature) { 
  return feature.set('area', feature.area(SCALE)) 
});

// getting all the vectors having area between 1e4 to 1e5
var Pixel_one = pixel_polys.filterMetadata('area', 'greater_than', 1e4);
var pixel_two = Pixel_one.filterMetadata('area', 'less_than', 1e5);

// adding identified hamlets on to the map
Map.addLayer(pixel_two, {color:'red'},'hamlets');

// printing the number of hamlets on console
print(pixel_two.size());

//Exporting the hamlets to the google drive
// Export.table.toDrive({
//   collection: pixel_two,
//   description: 'KorbaHamlets',
//   fileFormat: 'GEOJSON'
// });
Map.centerObject(image,10);
//721 rural areas
