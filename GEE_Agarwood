          [101.18103497013067, 4.466518587659399]]]),
    ndvi32 = ee.ImageCollection("LANDSAT/LC8_L1T_32DAY_NDVI"),
    gaharu = /* color: #98ff00 */ee.Geometry.Point([101.192874, 4.459094]),
    gopalnagar = /* color: #bf04c2 */ee.Geometry.Point([92.855356, 26.021318]),
    johor = /* color: #ff0000 */ee.Geometry.Point([103.851185, 1.515048]),
    lai = ee.ImageCollection("MODIS/006/MCD15A3H"),
    trainingArea = /* color: #39ffd1 */ee.Geometry.MultiPolygon(
        [[[[101.15009307861328, 4.469042856693324],
