    hansen = ee.Image("UMD/hansen/global_forest_change_2016_v1_4"),
    gopengarea = /* color: #0b4a8b */ee.Geometry.Polygon(
        [[[101.18511192375172, 4.458047068680267],
          [101.19661330097074, 4.456849074764683],
