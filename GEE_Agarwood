          "evenOdd": true
        }
      ],
      "coordinates": []
    }),
    l8 = ee.ImageCollection("LANDSAT/LC08/C01/T1_SR"),
    kurkut_basti = /* color: #4499d6 */ee.Geometry.Polygon(
        [[[92.8196289045344, 25.927483285930844],
          [92.84475842991708, 25.92810035493305],
          [92.84287156599669, 25.94221490992661],
          [92.81842818168138, 25.939824039573658]]]);
/***** End of imports. If edited, may not auto-convert in the playground. *****/
// ---------------------------------- Data Preparation -----------------------------------
// Merging 3 classes marked as Land (class 0), Agarwood areas (class 1) and Non-Agarwood areas (class 2)
var trainingFeatures = land.merge(agarwood).merge(nonagarwood);
// selecting 'B1', 'B2', 'B3', 'B4', 'B5' bands of Landsat 8 Dataset
var landsat8Bands = ['B1', 'B2', 'B3', 'B4', 'B5'];
// selecting 'Fpar', 'Lai' bands of Leaf Dataset
var leafBands = ['Fpar','Lai'];
// using Landsat 8 dataset
var l8imgs = ee.ImageCollection(l8).filterBounds(trainingFeatures).select(landsat8Bands).filterDate('2017-01-01','2017-12-31').sort('CLOUD_COVER');
var l8img1 = ee.Image(l8imgs.first());
print('Landsat8 Data:',l8img1)
Map.addLayer(l8img1,'','Landsat8 Data')
// using Leaf Area Index/FPAR dataset
var lai_image = ee.Image(lai.filterBounds(trainingFeatures).select(leafBands).filterDate('2017-01-01','2017-12-31').sort('CLOUD_COVER').first()); 
print('Leaf Data:',lai_image)
Map.addLayer(lai_image,'','Leaf Data')
// using Landsat 8 NDVI32 day composite dataset
var ndvi = ee.Image(ndvi32.filterBounds(trainingFeatures).select('NDVI').filterDate('2017-01-01','2017-12-31').sort('CLOUD_COVER').first()); 
print('NDVI Data:',ndvi)
Map.addLayer(ndvi,'','NDVI Data')
// using 'treecover2000' band from Hansen dataset
var tree = hansen.select('treecover2000');
print('Hansen Data:',tree)
Map.addLayer(tree,'','Hansen Data')
// adding all selected bands
var trainingImage = ee.Image(1)
    .addBands(l8img1)
    .addBands(lai_image)
    .addBands(ndvi)
    .addBands(tree);
print('Training Image:',trainingImage)
// prepare training image
var training = trainingImage.sampleRegions({
collection:trainingFeatures,
properties:['class'],
scale:30
});
// using CART classifier for training
var trained = ee.Classifier.cart().train({
  features:training,
  classProperty:'class',
  inputProperties:landsat8Bands
  });
// classification on training image
var classified = trainingImage.select(landsat8Bands).classify(trained);
var Kurkut_Basti = ee.Image(l8.filterBounds(kurkut_basti).filterDate('2017-01-01','2017-12-31').sort('CLOUD_COVER').first()); 
var classified2 = Kurkut_Basti.select(landsat8Bands).classify(trained);
Map.addLayer(classified2, {min:0, max:2, palette: ['red', 'green', 'blue']}, 'classified_kurkutBasti');
// --------------------------------- Accuracy Assessment -------------------------------------
// random train-test split of training data
var trainingTesting = training.randomColumn();    
// 60% data as training set
var trainingSet = trainingTesting.filter(ee.Filter.greaterThanOrEquals('random', 0.6));    
print('Train Set:',trainingSet)
// 40% data as testing set
var testingSet = trainingTesting.filter(ee.Filter.lessThanOrEquals('random', 0.4));    
print('Test Set:',testingSet)
// Classification using Train Set
var classification = ee.Classifier.cart().train({
  features: trainingSet, 
  classProperty: 'class', 
  inputProperties: landsat8Bands
});
// Confusion Matrix for Test Set
var confusionMatrix = ee.ConfusionMatrix(testingSet.classify(classification)
    .errorMatrix({
      actual: 'class', 
      predicted: 'classification'
    }));
print('Confusion Matrix:', confusionMatrix);
print('Accuracy:', confusionMatrix.accuracy().multiply(100));
// -------------------------------Exporting Data as CSV to Drive----------------------------
// Train set
Export.table.toDrive({
  collection: trainingSet,
  description: 'Agarwood_Train',
  fileFormat: "CSV"
});
// Test set
Export.table.toDrive({
  collection: testingSet,
  description: 'Agarwood_Test',
  fileFormat: "CSV"
});
