// print("Temperature",temp);
var fah = temp.multiply(1.8).add(32);
// print("Temperature in Fahreinheit",fah);
var ad=fah.multiply(2.04901523);
var ad1 = hum.multiply(10.14333127);
var ad2 =  fah.multiply(hum).multiply(0.22475541);
var ad3 = fah.multiply(fah).multiply(0.00683783);
var ad4 = hum.multiply(hum).multiply(0.05481717);
var ad5 = hum.multiply(fah).multiply(fah).multiply(0.00122874);
var ad6 = hum.multiply(hum).multiply(fah).multiply(0.00085282);
var ad7 = hum.multiply(hum).multiply(fah).multiply(fah).multiply(0.00000199);
var sub1  = ad2.add(ad3).add(ad4).add(ad7).add(42.379);
var add1  = ad.add(ad1).add(ad5).add(ad6);
var HISep  = add1.subtract(sub1);
//print("Heat Index Sep 2016",HISep);
//HI Oct
var ar1= ar.filterBounds(delhi).filterDate('2016-10-01','2016-10-30').select("relative_humidity_2m_above_ground").mosaic();
ar1 = ar1.clip(delhi);
Map.addLayer(ar1 ,'','Relative Humidity');
var ar2= ar.filterBounds(delhi).filterDate('2016-10-01','2016-10-30').select("temperature_2m_above_ground").mosaic();
ar2 = ar2.clip(delhi);
Map.addLayer(ar2 ,'','Temperature');
Map.centerObject(delhi,9);
var hum = ar1.reduceRegion(ee.Reducer.mean(), delhi, 500).get("relative_humidity_2m_above_ground");
var temp = ar2.reduceRegion(ee.Reducer.mean(), delhi, 500).get("temperature_2m_above_ground");
hum = ee.Number(hum);
temp = ee.Number(temp);
