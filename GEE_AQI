//HI for Jan
var ar1= ar.filterBounds(delhi).filterDate('2016-01-01','2016-01-31').select("relative_humidity_2m_above_ground").mosaic();
ar1 = ar1.clip(delhi);
Map.addLayer(ar1 ,'','Relative Humidity');
var ar2= ar.filterBounds(delhi).filterDate('2016-01-01','2016-01-31').select("temperature_2m_above_ground").mosaic();
ar2 = ar2.clip(delhi);
Map.addLayer(ar2 ,'','Temperature');
Map.centerObject(delhi,9);
var hum = ar1.reduceRegion(ee.Reducer.mean(), delhi, 500).get("relative_humidity_2m_above_ground");
var temp = ar2.reduceRegion(ee.Reducer.mean(), delhi, 500).get("temperature_2m_above_ground");
hum = ee.Number(hum);
temp = ee.Number(temp);
// print("Humidity",hum);
// print("Temperature",temp);
var fah = temp.multiply(1.8).add(32);
// print("Temperature in Fahreinheit",fah);
